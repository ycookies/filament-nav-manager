{
    "name": "ycookies/filament-nav-manager",
    "description": "A powerful navigation management package for Filament v4 that allows you to dynamically manage your Filament panel navigation menus",
    "keywords": [
        "ycookies",
        "laravel",
        "filament-nav-manager"
    ],
    "homepage": "https://github.com/ycookies/filament-nav-manager",
    "support": {
        "issues": "https://github.com/ycookies/filament-nav-manager/issues",
        "source": "https://github.com/ycookies/filament-nav-manager"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "eRic",
            "email": "3664839@qq.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Ycookies\\FilamentNavManager\\": "src/",
            "Ycookies\\FilamentNavManager\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ycookies\\FilamentNavManager\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ycookies\\FilamentNavManager\\FilamentNavManagerServiceProvider"
            ],
            "aliases": {
                "FilamentNavManager": "Ycookies\\FilamentNavManager\\Facades\\FilamentNavManager"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}